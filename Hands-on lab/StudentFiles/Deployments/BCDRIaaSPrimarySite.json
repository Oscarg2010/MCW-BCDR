{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "domainControllerDnsName": {
      "type": "string",
      "defaultValue": "bcdrdc1updateme",
      "minLength": 1
    }
  },
  "variables": {
    "GitHubScriptRepo": "microsoft/MCW-Business-continuity-and-disaster-recovery",
    "GitHubScriptRepoBranch": "master",
    "GitHubScriptRepoBranchURL": "[concat('https://raw.githubusercontent.com/', variables('GitHubScriptRepo'), '/', variables('GitHubScriptRepoBranch'), '/Hands-on lab/NestedDeployments/')]",
    "GitHubMediaRepoBranchURL": "[concat('https://media.githubusercontent.com/media/', variables('GitHubScriptRepo'), '/', variables('GitHubScriptRepoBranch'), '/Hands-on lab/NestedDeployments/')]",
    "BCDRVnetUpdateDNSTemplateURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'BCDRVnetWithDNS.json')]",
    "BCDRIaaSSecondarySiteTemplateURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'BCDRIaaSSecondarySite.json')]",
    "BCDRVnetIaaSSecondaryVnetTemplateURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'BCDRIaaSSecondaryVnet.json')]",
    "secondResourceGroup": "BCDRIaaSSecondarySite",
    "BCDRVNetName": "BCDRVNET",
    "BCDRVNetPrefix": "10.0.0.0/16",
    "BCDRVNetSubnet2Name": "WEB",
    "BCDRVNetSubnet2Prefix": "10.0.1.0/24",
    "BCDRVNetSubnet3Name": "DATA",
    "BCDRVNetSubnet3Prefix": "10.0.2.0/24",
    "BCDRVNetSubnet4Name": "AD",
    "BCDRVNetSubnet4Prefix": "10.0.3.0/24",
    "InternalLoadBalancerName": "SQLAOGILB",
    "InternalLoadBalancerID": "[resourceId('Microsoft.Network/loadBalancers',variables('InternalLoadBalancerName'))]",
    "ExternalLoadBalancerName": "WWWEXTLB",
    "ExternalLoadBalancerPublicIPName": "WWWEXTLB-PIP",
    "ExternalLoadBalancerPublicIPDNSName": "[concat('bcdrprimarysitelb', uniqueString(resourceGroup().id))]",
    "ExternalLoadBalancerID": "[resourceId('Microsoft.Network/loadBalancers',variables('ExternalLoadBalancerName'))]",
    "ExternalLoadBalancerfrontEndIPConfigID": "[concat(variables('ExternalLoadBalancerID'),'/frontendIPConfigurations/LoadBalancerFrontEnd')]",
    "ExternalLoadBalancerPoolID": "[concat(variables('ExternalLoadBalancerID'),'/backendAddressPools/BackendPool1')]",
    "ExternalLoadBalancerProbeID": "[concat(variables('ExternalLoadBalancerID'),'/probes/tcpProbe')]",
    "ExternalLoadbalancerFrontEndIPConfigID": "[concat(variables('ExternalLoadbalancerID'),'/frontendIPConfigurations/LoadBalancerFrontEnd')]",
    "bcdrstorageName": "[concat('bcdrstorage', uniqueString(resourceGroup().id))]",
    "bcdrstorageType": "Premium_LRS",
    "BCDRDC1Name": "BCDRDC1",
    "BCDRDC1AdminUserName": "mcwadmin",
    "BCDRDC1AdminPassword": "demo@pass123",
    "BCDRDC1ImagePublisher": "MicrosoftWindowsServer",
    "BCDRDC1ImageOffer": "WindowsServer",
    "BCDRDC1WindowsOSVersion": "2016-Datacenter",
    "BCDRDC1OSDiskName": "BCDRDC1OSDisk",
    "BCDRDC1VmSize": "Standard_F4",
    "BCDRDC1VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
    "BCDRDC1SubnetRef": "[concat(variables('BCDRDC1VnetID'), '/subnets/', variables('BCDRVNetSubnet4Name'))]",
    "BCDRDC1NicName": "[concat(variables('BCDRDC1Name'), '-NIC')]",
    "BCDRDC1-PUBIPName": "BCDRDC1-PIP",
    "ADNSGName": "ADNSG",
    "DEFAULTNSGName": "Default",
    "SQLVM1Name": "SQLVM1",
    "SQLVM1AdminUserName": "mcwadmin",
    "SQLVM1AdminPassword": "demo@pass123",
    "SQLVM1ImagePublisher": "MicrosoftSQLServer",
    "SQLVM1ImageOffer": "SQL2017-WS2016",
    "SQLVM1WindowsOSVersion": "SQLDEV",
    "SQLVM1OSDiskName": "SQLVM1OSDisk",
    "SQLVM1VmSize": "Standard_F4",
    "SQLVM1VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
    "SQLVM1SubnetRef": "[concat(variables('SQLVM1VnetID'), '/subnets/', variables('BCDRVNetSubnet3Name'))]",
    "SQLVM1PubIPName": "[concat(variables('SQLVM1Name'), '-IP')]",
    "SQLVM1NicName": "[concat(variables('SQLVM1Name'), '-NIC')]",
    "SQLVM2Name": "SQLVM2",
    "SQLVM2AdminUserName": "mcwadmin",
    "SQLVM2AdminPassword": "demo@pass123",
    "SQLVM2ImagePublisher": "MicrosoftSQLServer",
    "SQLVM2ImageOffer": "SQL2017-WS2016",
    "SQLVM2WindowsOSVersion": "SQLDEV",
    "SQLVM2OSDiskName": "SQLVM2OSDisk",
    "SQLVM2VmSize": "Standard_F4",
    "SQLVM2VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
    "SQLVM2SubnetRef": "[concat(variables('SQLVM2VnetID'), '/subnets/', variables('BCDRVNetSubnet3Name'))]",
    "SQLVM2PubIPName": "[concat(variables('SQLVM2Name'), '-IP')]",
    "SQLVM2NicName": "[concat(variables('SQLVM2Name'), '-NIC')]",
    "WEBVM1Name": "WEBVM1",
    "WEBVM1AdminUserName": "mcwadmin",
    "WEBVM1AdminPassword": "demo@pass123",
    "WEBVM1ImagePublisher": "MicrosoftWindowsServer",
    "WEBVM1ImageOffer": "WindowsServer",
    "WEBVM1WindowsOSVersion": "2016-Datacenter",
    "WEBVM1OSDiskName": "WEBVM1OSDisk",
    "WEBVM1VmSize": "Standard_F4",
    "WEBVM1VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
    "WEBVM1SubnetRef": "[concat(variables('WEBVM1VnetID'), '/subnets/', variables('BCDRVNetSubnet2Name'))]",
    "WEBVM1StorageAccountContainerName": "vhds",
    "WEBVM1NicName": "[concat(variables('WEBVM1Name'), '-NIC')]",
    "WEBVM2Name": "WEBVM2",
    "WEBVM2AdminUserName": "mcwadmin",
    "WEBVM2AdminPassword": "demo@pass123",
    "WEBVM2ImagePublisher": "MicrosoftWindowsServer",
    "WEBVM2ImageOffer": "WindowsServer",
    "WEBVM2WindowsOSVersion": "2016-Datacenter",
    "WEBVM2OSDiskName": "WEBVM2OSDisk",
    "WEBVM2VmSize": "Standard_F4",
    "WEBVM2VnetID": "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
    "WEBVM2SubnetRef": "[concat(variables('WEBVM2VnetID'), '/subnets/', variables('BCDRVNetSubnet2Name'))]",
    "WEBVM2StorageAccountContainerName": "vhds",
    "WEBVM2NicName": "[concat(variables('WEBVM2Name'), '-NIC')]",
    "WEBVM2JoinDomainScriptFolder": ".",
    "WEBVM2JoinDomainScriptFileName": "WEBVM2JoinDomain.ps1",
    "WEBVM2JoinDomainURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'CustomScripts/', variables('WEBVM2JoinDomainScriptFileName'))]",
    "NewForestScriptFolder": ".",
    "NewForestScriptFileName": "NewForest.ps1",
    "NewForestURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'CustomScripts/', variables('NewForestScriptFileName'))]",
    "SQLVM1ConfigArchiveFolder": ".",
    "SQLVM1ConfigArchiveFileName": "SQLVM1Config.zip",
    "SQLVM1ConfigURL": "[concat(variables('GitHubMediaRepoBranchURL'), 'CustomScripts/', variables('SQLVM1ConfigArchiveFileName'))]",
    "SQLVM1JoinDomainScriptFolder": ".",
    "SQLVM1JoinDomainScriptFileName": "SQLVM1JoinDomain.ps1",
    "SQLVM1JoinDomainURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'CustomScripts/', variables('SQLVM1JoinDomainScriptFileName'))]",
    "SQLVM2ConfigArchiveFolder": ".",
    "SQLVM2ConfigArchiveFileName": "SQLVM2Config.zip",
    "SQLVM2ConfigURL": "[concat(variables('GitHubMediaRepoBranchURL'), 'CustomScripts/', variables('SQLVM2ConfigArchiveFileName'))]",
    "SQLVM2JoinDomainScriptFolder": ".",
    "SQLVM2JoinDomainScriptFileName": "SQLVM2JoinDomain.ps1",
    "SQLVM2JoinDomainURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'CustomScripts/', variables('SQLVM2JoinDomainScriptFileName'))]",
    "WEBVM1JoinDomainScriptFolder": ".",
    "WEBVM1JoinDomainScriptFileName": "WEBVM1JoinDomain.ps1",
    "WEBVM1JoinDomainURL": "[concat(variables('GitHubScriptRepoBranchURL'), 'CustomScripts/', variables('WEBVM1JoinDomainScriptFileName'))]",
    "SecondaryVnetResourceGroup": "BCDRIaaSSecondarySite",
    "vNet2Name": "BCDRFOVNET",
    "vNet1tovNet2PeeringName": "[concat(variables('BCDRVNetName'), '/peering-to-remote-vnet')]"
  },
  "resources": [
    {
      "type": "Microsoft.Network/networkSecurityGroups",
      "apiVersion": "2018-12-01",
      "name": "[variables('ADNSGName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "securityRules": [
          {
            "name": "RDP",
            "properties": {
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "3389",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 100,
              "direction": "Inbound"
            }
          }
        ],
        "defaultSecurityRules": [
          {
            "name": "AllowVnetInBound",
            "properties": {
              "description": "Allow inbound traffic from all VMs in VNET",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "VirtualNetwork",
              "destinationAddressPrefix": "VirtualNetwork",
              "access": "Allow",
              "priority": 65000,
              "direction": "Inbound"
            }
          },
          {
            "name": "AllowAzureLoadBalancerInBound",
            "properties": {
              "description": "Allow inbound traffic from azure load balancer",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "AzureLoadBalancer",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 65001,
              "direction": "Inbound"
            }
          },
          {
            "name": "DenyAllInBound",
            "properties": {
              "description": "Deny all inbound traffic",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Deny",
              "priority": 65500,
              "direction": "Inbound"
            }
          },
          {
            "name": "AllowVnetOutBound",
            "properties": {
              "description": "Allow outbound traffic from all VMs to all VMs in VNET",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "VirtualNetwork",
              "destinationAddressPrefix": "VirtualNetwork",
              "access": "Allow",
              "priority": 65000,
              "direction": "Outbound"
            }
          },
          {
            "name": "AllowInternetOutBound",
            "properties": {
              "description": "Allow outbound traffic from all VMs to Internet",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "Internet",
              "access": "Allow",
              "priority": 65001,
              "direction": "Outbound"
            }
          },
          {
            "name": "DenyAllOutBound",
            "properties": {
              "description": "Deny all outbound traffic",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Deny",
              "priority": 65500,
              "direction": "Outbound"
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.Network/networkSecurityGroups",
      "apiVersion": "2018-12-01",
      "name": "[variables('DEFAULTNSGName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "securityRules": [
          {
            "name": "Port_80",
            "properties": {
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "80",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 100,
              "direction": "Inbound",
              "sourcePortRanges": [],
              "destinationPortRanges": [],
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "Port_443",
            "properties": {
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "443",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 110,
              "direction": "Inbound",
              "sourcePortRanges": [],
              "destinationPortRanges": [],
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          }
        ],
        "defaultSecurityRules": [
          {
            "name": "AllowVnetInBound",
            "properties": {
              "description": "Allow inbound traffic from all VMs in VNET",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "VirtualNetwork",
              "destinationAddressPrefix": "VirtualNetwork",
              "access": "Allow",
              "priority": 65000,
              "direction": "Inbound"
            }
          },
          {
            "name": "AllowAzureLoadBalancerInBound",
            "properties": {
              "description": "Allow inbound traffic from azure load balancer",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "AzureLoadBalancer",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 65001,
              "direction": "Inbound"
            }
          },
          {
            "name": "DenyAllInBound",
            "properties": {
              "description": "Deny all inbound traffic",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Deny",
              "priority": 65500,
              "direction": "Inbound"
            }
          },
          {
            "name": "AllowVnetOutBound",
            "properties": {
              "description": "Allow outbound traffic from all VMs to all VMs in VNET",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "VirtualNetwork",
              "destinationAddressPrefix": "VirtualNetwork",
              "access": "Allow",
              "priority": 65000,
              "direction": "Outbound"
            }
          },
          {
            "name": "AllowInternetOutBound",
            "properties": {
              "description": "Allow outbound traffic from all VMs to Internet",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "Internet",
              "access": "Allow",
              "priority": 65001,
              "direction": "Outbound"
            }
          },
          {
            "name": "DenyAllOutBound",
            "properties": {
              "description": "Deny all outbound traffic",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Deny",
              "priority": 65500,
              "direction": "Outbound"
            }
          }
        ]
      }
    },
    {
      "name": "[variables('BCDRVNetName')]",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2018-12-01",
      "dependsOn": [
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('ADNSGName'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', variables('DEFAULTNSGName'))]"
      ],
      "tags": {
        "displayName": "[variables('BCDRVNetName')]"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('BCDRVNetPrefix')]"
          ]
        }
      },
      "resources": [
        {
          "type": "subnets",
          "apiVersion": "2018-12-01",
          "name": "[variables('BCDRVNetSubnet4Name')]",
          "dependsOn": [
            "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]"
          ],
          "properties": {
            "addressPrefix": "[variables('BCDRVNetSubnet4Prefix')]",
            "networkSecurityGroup": {
              "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('ADNSGName'))]"
            },
            "serviceEndpoints": [],
            "delegations": []
          }
        },
        {
          "type": "subnets",
          "apiVersion": "2018-12-01",
          "name": "[variables('BCDRVNetSubnet3Name')]",
          "dependsOn": [
            "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
            "[resourceId('Microsoft.Network/virtualNetworks/subnets', variables('BCDRVNetName'), variables('BCDRVNetSubnet4Name'))]"
          ],
          "properties": {
            "addressPrefix": "[variables('BCDRVNetSubnet3Prefix')]",
            "networkSecurityGroup": {
              "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('DEFAULTNSGName'))]"
            },
            "serviceEndpoints": [],
            "delegations": []
          }
        },
        {
          "type": "subnets",
          "apiVersion": "2018-12-01",
          "name": "[variables('BCDRVNetSubnet2Name')]",
          "dependsOn": [
            "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
            "[resourceId('Microsoft.Network/virtualNetworks/subnets', variables('BCDRVNetName'), variables('BCDRVNetSubnet3Name'))]"
          ],
          "properties": {
            "addressPrefix": "[variables('BCDRVNetSubnet2Prefix')]",
            "networkSecurityGroup": {
              "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('DEFAULTNSGName'))]"
            },
            "serviceEndpoints": [],
            "delegations": []
          }
        }
      ]
    },
    {
      "name": "BCDRVnetIaaSSecondaryVnet",
      "type": "Microsoft.Resources/deployments",
      "resourceGroup": "[variables('secondResourceGroup')]",
      "apiVersion": "2017-05-10",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('BCDRVnetIaaSSecondaryVnetTemplateURL')]",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "apiVersion": "2018-08-01",
      "type": "Microsoft.Network/virtualNetworks/virtualNetworkPeerings",
      "name": "[variables('vNet1tovNet2PeeringName')]",
      "location": "[resourceGroup().location]",
      "comments": "This is the peering from BCDRVNET to BCDRFOVNET",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
        "[concat('Microsoft.Resources/deployments/', 'BCDRVnetIaaSSecondaryVnet')]"
      ],
      "properties": {
        "allowVirtualNetworkAccess": "true",
        "allowForwardedTraffic": "false",
        "allowGatewayTransit": "false",
        "useRemoteGateways": "false",
        "remoteVirtualNetwork": {
          "id": "[resourceId(variables('SecondaryVnetResourceGroup'), 'Microsoft.Network/virtualNetworks', variables('vnet2Name'))]"
        }
      }
    },
    {
      "name": "[variables('bcdrstorageName')]",
      "type": "Microsoft.Storage/storageAccounts",
      "location": "[resourceGroup().location]",
      "apiVersion": "2017-10-01",
      "sku": {
        "name": "[variables('bcdrstorageType')]"
      },
      "dependsOn": [],
      "tags": {
        "displayName": "bcdrstorage"
      },
      "kind": "Storage"
    },
    {
      "name": "[variables('BCDRDC1NicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[resourceGroup().location]",
      "apiVersion": "2017-10-01",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
        "[resourceId('Microsoft.Network/publicIPAddresses', variables('BCDRDC1-PUBIPName'))]",
        "[resourceId('Microsoft.Network/virtualNetworks/subnets', variables('BCDRVNetName'), variables('BCDRVNetSubnet4Name'))]"
      ],
      "tags": {
        "displayName": "BCDRDC1Nic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAllocationMethod": "Dynamic",
              "subnet": {
                "id": "[variables('BCDRDC1SubnetRef')]"
              },
              "publicIPAddress": {
                "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('BCDRDC1-PUBIPName'))]"
              }
            }
          }
        ]
      }
    },
    {
      "name": "[variables('BCDRDC1Name')]",
      "type": "Microsoft.Compute/virtualMachines",
      "location": "[resourceGroup().location]",
      "apiVersion": "2018-10-01",
      "dependsOn": [
        "[resourceId('Microsoft.Network/networkInterfaces', variables('BCDRDC1NicName'))]"
      ],
      "tags": {
        "displayName": "BCDRDC1"
      },
      "properties": {
        "hardwareProfile": {
          "vmSize": "[variables('BCDRDC1VmSize')]"
        },
        "osProfile": {
          "computerName": "[variables('BCDRDC1Name')]",
          "adminUsername": "[variables('BCDRDC1AdminUsername')]",
          "adminPassword": "[variables('BCDRDC1AdminPassword')]"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "[variables('BCDRDC1ImagePublisher')]",
            "offer": "[variables('BCDRDC1ImageOffer')]",
            "sku": "[variables('BCDRDC1WindowsOSVersion')]",
            "version": "latest"
          },
          "osDisk": {
            "createOption": "FromImage",
            "diskSizeGB": 1023
          },
          "dataDisks": [
            {
              "diskSizeGB": 1023,
              "lun": 0,
              "createOption": "Empty"
            }
          ]
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('BCDRDC1NicName'))]"
            }
          ]
        }
      },
      "resources": [
        {
          "name": "NewForest",
          "type": "extensions",
          "location": "[resourceGroup().location]",
          "apiVersion": "2017-12-01",
          "dependsOn": [
            "[resourceId('Microsoft.Compute/virtualMachines', variables('BCDRDC1Name'))]"
          ],
          "tags": {
            "displayName": "NewForest"
          },
          "properties": {
            "publisher": "Microsoft.Compute",
            "type": "CustomScriptExtension",
            "typeHandlerVersion": "1.4",
            "autoUpgradeMinorVersion": true,
            "settings": {
              "fileUris": [
                "[concat(variables('NewForestURL'))]"
              ],
              "commandToExecute": "[concat('powershell -ExecutionPolicy Unrestricted -File ', variables('NewForestScriptFolder'), '/', variables('NewForestScriptFileName'))]"
            }
          }
        }
      ]
    },
    {
      "name": "[variables('BCDRDC1-PUBIPName')]",
      "type": "Microsoft.Network/publicIPAddresses",
      "location": "[resourceGroup().location]",
      "apiVersion": "2018-08-01",
      "dependsOn": [],
      "tags": {
        "displayName": "BCDRDC1-PUBIP"
      },
      "properties": {
        "publicIPAllocationMethod": "dynamic",
        "dnsSettings": {
          "domainNameLabel": "[parameters('domainControllerDnsName')]"
        }
      }
    },
    {
      "name": "BCDRVnetUpdateDNS",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2017-05-10",
      "dependsOn": [
        "[resourceId('Microsoft.Compute/virtualMachines/extensions', variables('BCDRDC1Name'), 'NewForest')]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('BCDRVnetUpdateDNSTemplateURL')]",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "name": "[variables('SQLVM1NicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[resourceGroup().location]",
      "apiVersion": "2017-10-01",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', variables('BCDRVNetName'))]",
        "[concat('Microsoft.Resources/deployments/', 'BCDRVnetUpdateDNS')]",
        "[resourceId('Microsoft.Network/virtualNetworks/subnets', variables('BCDRVNetName'), variables('BCDRVNetSubnet3Name'))]",
        "[resourceId('Microsoft.Network/publicIPAddresses', variables('SQLVM1PubIPName'))]"
      ],
      "tags": {
        "displayName": "SQLVM1Nic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAllocationMethod": "Dynamic",
              "publicIPAddress": {
                "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('SQLVM1PubIPName'))]"
              },
              "subnet": {
                "id": "[variables('SQLVM1SubnetRef')]"
              },
